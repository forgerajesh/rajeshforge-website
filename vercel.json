{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/contact", "destination": "/pages/contact", "permanent": true },
    { "source": "/services", "destination": "/pages/services", "permanent": true },
    { "source": "/about", "destination": "/pages/about", "permanent": true },
    { "source": "/architects-hub", "destination": "/pages/architects-hub", "permanent": true },
    { "source": "/case-studies", "destination": "/pages/case-studies", "permanent": true },
    { "source": "/insights", "destination": "/pages/insights", "permanent": true },
    { "source": "/thank-you", "destination": "/pages/thank-you", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" }
      ]
    }
  ]
}
